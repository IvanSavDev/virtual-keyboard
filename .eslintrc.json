{
  "env": {
    "browser": true,
    "es2021": true
  },
  "ignorePatterns": ["dist", "node_modules", "webpack.*"],
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  }
}
